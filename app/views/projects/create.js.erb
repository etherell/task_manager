<% if @project.errors.empty? %> 
  $('#projects').prepend('<%= j render @project %>');
  $('.new-project-form').val('');
  $('.new-project-form').removeClass('is-invalid');
  setTimeout(setFlatpickr, 500);
<% else %>
  $('.new-project-form').addClass('is-invalid');
<% end %>
<%= render partial: 'shared/toastr' %>
